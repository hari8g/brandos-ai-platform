{
  "name": "brandos-frontend",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:standalone": "vite --config vite.standalone.config.ts",
    "type-check": "tsc --noEmit -p tsconfig.build.json",
    "build": "npm run type-check && vite build --config vite.config.vercel.ts",
    "build:local": "npm run type-check && vite build",
    "preview": "vite preview",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --report-unused-disable-directives --max-warnings 0"
  },
  "eslintConfig": {
    "overrideConfigFile": "eslint.config.js"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-slot": "^1.2.3",
    "axios": "^1.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "lottie-react": "^2.4.1",
    "lucide-react": "^0.514.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scroll": "^1.9.3",
    "react-simple-typewriter": "^5.0.1",
    "recharts": "^3.1.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.0",
    "@tailwindcss/vite": "^4.1.9",
    "@types/html2canvas": "^0.5.35",
    "@types/node": "^24.0.0",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@types/react-scroll": "^1.8.10",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.30.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.0.0",
    "postcss": "^8.5.4",
    "tailwindcss": "^4.1.9",
    "tw-animate-css": "^1.3.4",
    "typescript": "~5.8.3",
    "vite": "^6.3.5"
  }
}
