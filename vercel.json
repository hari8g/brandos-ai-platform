{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "api/basic.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/assess.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/formulate.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/basic",
      "dest": "/api/basic.py"
    },
    {
      "src": "/api/v1/query/assess",
      "dest": "/api/assess.py"
    },
    {
      "src": "/api/v1/formulation/generate",
      "dest": "/api/formulate.py"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ]
} 